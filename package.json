{
  "name": "module-base",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:clean": "npm run clean && vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "preview:clean": "npm run clean && npm run build && vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "rm -rf dev-dist build .svelte-kit",
    "build:pwa": "npm run clean && vite build",
    "test": "playwright test",
    "test:dev": "playwright test --config=playwright.config.dev.ts",
    "test:prod": "playwright test --config=playwright.config.prod.ts",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --project=chromium --headed --timeout=60000",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:codegen": "playwright codegen",
    "test:auth": "playwright test tests/auth/authentication-flow.spec.ts tests/auth/real-auth.spec.ts",
    "test:dashboard": "playwright test tests/dashboard/dashboard-functionality.spec.ts",
    "test:e2e": "playwright test tests/e2e/complete-user-journey.spec.ts",
    "test:pwa-e2e": "playwright test tests/pwa/pwa-functionality.spec.ts",
    "test:core": "playwright test tests/core/application-core.spec.ts",
    "test:complete": "playwright test tests/run-complete-test-suite.spec.ts",
    "test:all": "playwright test",
    "test:comprehensive": "playwright test tests/comprehensive-e2e-suite.spec.ts",
    "test:modules": "playwright test tests/modules/module-system.spec.ts",
    "test:performance": "playwright test tests/performance-security.spec.ts",
    "test:runner": "playwright test tests/run-all-tests.spec.ts",
    "test:real-auth": "playwright test tests/auth/real-auth.spec.ts",
    "test:postgres": "playwright test tests/auth/real-auth.spec.ts --project=chromium",
    "test:helper": "bash scripts/test-helper.sh",
    "setup:postgres": "bash scripts/setup-postgres.sh",
    "backup:db": "bash scripts/backup.sh",
    "test:smoke": "playwright test tests/run-complete-test-suite.spec.ts --project=chromium",
    "test:mobile": "playwright test --project=mobile-chrome,mobile-safari",
    "test:desktop": "playwright test --project=chromium,firefox,webkit",
    "docker:verify": "bash scripts/verify-templates-docker.sh",
    "docker:dev": "npm run dev -- --host 0.0.0.0 --port 5173",
    "test:pwa": "node test-pwa.js",
    "test:pwa-setup": "node test-pwa-setup.js",
    "test:pwa-simple": "node test-pwa-simple.js"
  },
  "devDependencies": {
    "@lucide/svelte": "^0.525.0",
    "@playwright/test": "^1.54.1",
    "@sveltejs/adapter-auto": "^6.0.2",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tsconfig/svelte": "^5.0.4",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "postcss": "^8.5.6",
    "sharp": "^0.34.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.0.0",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.0.2",
    "workbox-window": "^7.3.0"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@supabase/supabase-js": "^2.39.0",
    "@sveltejs/adapter-node": "^5.2.14",
    "@tailwindcss/vite": "^4.1.11",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.7"
  }
}
