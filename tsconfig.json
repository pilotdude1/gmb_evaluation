{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "types": ["@playwright/test"]
  },
  "include": [
    ".svelte-kit/types/**/$types.d.ts",
    ".svelte-kit/ambient.d.ts",
    ".svelte-kit/non-ambient.d.ts",
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.svelte"
  ],
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "playwright-report",
    "test-results",
    "static",
    "tests",
    "markdown-files",
    "sql-files",
    "scripts",
    "grafana",
    "prometheus",
    "postgres",
    "dev-dist",
    ".svelte-kit/output",
    "docker-compose*.yml",
    "Dockerfile*",
    "*.sh",
    "*.md",
    "*.sql",
    "*.yml",
    "*.yaml",
    "*.json",
    "*.lock",
    "*.log",
    "*.tmp",
    "*.cache",
    "*.swp",
    "*.swo",
    "*.bak",
    "*.orig",
    "*.rej",
    "*.pid",
    "*.seed",
    "*.pid.lock",
    "npm-debug.log*",
    "yarn-debug.log*",
    "yarn-error.log*",
    "lerna-debug.log*",
    ".pnpm-debug.log*",
    "coverage",
    ".nyc_output",
    ".coverage",
    ".nyc_output",
    "lib-cov",
    "coverage.json",
    "*.lcov",
    ".grunt",
    ".bower-cache",
    ".bower-components",
    ".bower-registry",
    ".bower-tmp",
    ".bower",
    "bower_components",
    ".node_repl_history",
    "*.tgz",
    ".yarn-integrity",
    ".env.local",
    ".env.development.local",
    ".env.test.local",
    ".env.production.local",
    "*.tsbuildinfo",
    ".next",
    ".nuxt",
    ".vuepress/dist",
    ".serverless",
    ".fusebox/",
    ".dynamodb/",
    ".tern-port",
    ".vscode-test",
    ".yarn/cache",
    ".yarn/unplugged",
    ".yarn/build-state.yml",
    ".yarn/install-state.gz",
    ".pnp.*"
  ]
  // Path aliases are handled by https://svelte.dev/docs/kit/configuration#alias
  // except $lib which is handled by https://svelte.dev/docs/kit/configuration#files
  //
  // If you want to overwrite includes/excludes, make sure to copy over the relevant includes/excludes
  // from the referenced tsconfig.json - TypeScript does not merge them in
}
